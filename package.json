{
  "name": "feedcatv2",
  "displayName": "Feed Cat - Coding Companion",
  "description": "A cute cat companion that lives in your VSCode. Feed it by typing code!",
  "version": "0.0.2",
  "publisher": "lannes",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "feedcat.show",
        "title": "Feed Cat: Show Cat"
      },
      {
        "command": "feedcat.reset",
        "title": "Feed Cat: Reset Counter"
      },
      {
        "command": "feedcat.spawnFish",
        "title": "Feed Cat: Spawn Fish"
      }
    ],
    "views": {
      "explorer": [
        {
          "type": "webview",
          "id": "feedcat.catView",
          "name": "üê± Cat Companion",
          "visibility": "visible"
        }
      ]
    },
    "configuration": {
      "title": "Feed Cat",
      "properties": {
        "feedcat.fishThreshold": {
          "type": "number",
          "default": 1000,
          "description": "Number of keystrokes before a fish appears"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.52.0",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
